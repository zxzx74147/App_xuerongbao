<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="android.view.View" />

        <variable
            name="handler"
            type="com.wazxb.zhuxuebao.moudles.payback.PaybackActivity" />


    </data>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/view_default_bg"
        android:fitsSystemWindows="true"
        android:orientation="vertical">

        <com.zxzx74147.devlib.widget.TitleBar
            android:layout_width="match_parent"
            android:layout_height="@dimen/default_gap_110"
            app:text="@string/payback" />

        <com.zxzx74147.devlib.widget.list.ZXRecycleView
            android:id="@+id/list"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </LinearLayout>
</layout>